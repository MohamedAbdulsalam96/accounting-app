 "css": ".print__parent {\n    font-size: 15px;\n}\n.print__header {\n    padding: 10px;\n}\n.print__rows {\n    margin: 20px;\n}\n.print__col {\n    margin-top: 10px;\n    margin-bottom: 10px;\n}",
 "custom_format": 1,
 "default_print_language": "en",
