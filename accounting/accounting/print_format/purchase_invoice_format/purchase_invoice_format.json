 "doctype": "Print Format",
 "font": "Default",
 "html": "<div class=\"print__parent\">\n    <div class=\"border-bottom print__header\">\n        <h1 class=\"float-right\">{{ _(\"Purchase Invoice\") }}</h1>\n        <div class=\"float-right text-muted\">{{doc.name}}</div>\n    </div>\n    <div class='row print__rows'>\n        <div class='col-6'></div>\n        <div class='col-6'>\n            <div class='row'>\n                <div class='col-sm-6 print__col'>\n                    <b>\n                        {{ _(\"Supplier:\") }}\n                    </b>\n                    {{ doc.party }}\n                </div>\n                <div class='col-sm-6 print__col'>\n                    <b>\n                        {{ _(\"Series:\") }}\n                    </b>\n                    {{ doc.naming_series }}\n                </div>\n            </div>\n            <div class='row'>\n                <div class='col-sm-6 print__col'>\n                    <b>\n                        {{ _(\"Posting Date:\") }}\n                    </b>\n                    {{doc.posting_date}}\n                </div>\n                <div class='col-sm-6 print__col'>\n                    <b>\n                        {{ _(\"Payment Due Date:\") }}\n                    </b>\n                    {{ doc.payment_due_date}}\n                </div>\n            </div>\n        </div>\n    </div>\n    <table class=\"table table-bordered\">\n        <thead>\n            <tr>\n                <th>{{ _(\"#\")}}</th>\n                <th>{{ _(\"Item\") }}</th>\n                <th class='text-right'>{{ _(\"Qty\") }}</th>\n                <th class='text-right'>{{ _(\"Rate\") }}</th>\n                <th class='text-right'>{{ _(\"Amount\") }}</th>\n            </tr>\n        </thead>\n        <tbody>\n            {% for row in doc.items %}\n            <tr>\n                <td class='text-right'>{{ row.idx }}</td>\n                <td class='text-right'>{{ row.item }}</td>\n                <td class='text-right'>{{ row.qty }}</td>\n                <td class='text-right'>{{ frappe.format(row.rate, 'Currency') }}</td>\n                <td class='text-right'>{{ frappe.format(row.amount, 'Currency') }}</td>\n            </tr>\n            {% endfor %}\n        </tbody>\n    </table>\n    <div class='row print__rows'>\n        <div class='col-6'></div>\n        <div class='col-6'>\n            <div class='row'>\n                <div class='col-sm-6 print__col'>\n                    <b>\n                        {{ _(\"Total Quantity:\") }}\n                    </b>\n                    {{ frappe.format(doc.total_quantity, 'Currency')}}\n                </div>\n                <div class='col-sm-6 print__col'>\n                    <b>\n                        {{ _(\"Total Amount:\") }}\n                    </b>\n                    {{ frappe.format(doc.total_amount, 'Currency')}}\n                </div>\n            </div>\n            <div class='row'>\n                <div class='col-sm-6 print__col'>\n                    <b>\n                        {{ _(\"Credit To:\") }}\n                    </b>\n                    {{doc.credit_to}}\n                </div>\n                <div class='col-sm-6 print__col'>\n                    <b>\n                        {{ _(\"Expense Account:\") }}\n                    </b>\n                    {{ doc.expense_account}}\n                </div>\n            </div>\n        </div>\n    </div>\n</div>",
